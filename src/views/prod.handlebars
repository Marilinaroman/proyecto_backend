<div class="d-flex column">
    {{#each producto}}
        <img class="rounded" src={{this.url}} alt={{this.nombre}}>
        <div >
            <h1>{{this.nombre}}</h1>
            <p>${{this.precio}}</p>
            <input type="number">
            <button class="btn btn-primary">Agregar al carrito</button>
        </div>
    {{/each}}
</div>
